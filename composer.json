{
    "name": "oro/phpstan-rules",
    "description": "A set of additional PHPStan rules used in Oro products.",
    "license": "MIT",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "https://oroinc.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {"": "src/"}
    },
    "require": {
        "phpstan/phpstan": "~0.12.65",
        "phpstan/phpstan-doctrine": "^0.12.26",
        "ocramius/proxy-manager": "2.2.*"
    }
}
